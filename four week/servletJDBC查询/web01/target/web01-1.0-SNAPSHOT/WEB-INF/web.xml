<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>web02</display-name>
    <servlet>
        <servlet-name>selectOneEmp</servlet-name>
        <servlet-class>com.example.web01.HelloServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet</servlet-name>
        <servlet-class>com.exampleweb02.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Servlet</servlet-name>
        <url-pattern>/selectOne</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>selectOneEmp</servlet-name>
        <url-pattern>/Hello</url-pattern>
    </servlet-mapping>
</web-app>